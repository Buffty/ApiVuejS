{"ast":null,"code":"import InitHome from './components/InitHome.vue';\nimport LoginUser from './components/LoginUser.vue';\nexport default {\n  name: 'App',\n  components: {\n    InitHome,\n    LoginUser\n  },\n  data: () => ({\n    dModalLogin: false,\n    dMenuLateral: false,\n    dListaLateral: [{\n      title: 'Series',\n      value: 0\n    }, {\n      title: 'Peliculas',\n      value: 1\n    }, {\n      title: 'Categorias',\n      value: 2,\n      items: [{\n        'title': 'Terror',\n        value: 0\n      }, {\n        'title': 'Comedia',\n        value: 1\n      }, {\n        'title': 'Drama',\n        value: 2\n      }]\n    }]\n  }),\n  methods: {\n    fOpenLoginUser() {\n      this.dModalLogin = true;\n    },\n    fCloseLogin() {\n      this.dModalLogin = false;\n    },\n    fSelectTipo(tipo) {\n      console.log(\"Tipo\");\n      console.log(tipo);\n    }\n  }\n};","map":{"version":3,"mappings":"AAwCA;AACA;AAEA;EACAA;EAEAC;IACAC;IACAC;EACA;EAEAC;IACAC;IACAC;IACAC,gBACA;MAAAC;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;MACAC,QACA;QAAA;QAAAD;MAAA,GACA;QAAA;QAAAA;MAAA,GACA;QAAA;QAAAA;MAAA;IAEA;EAEA;EAEAE;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACAC;MACAA;IACA;EACA;AACA","names":["name","components","InitHome","LoginUser","data","dModalLogin","dMenuLateral","dListaLateral","title","value","items","methods","fOpenLoginUser","fCloseLogin","fSelectTipo","console"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app color=\"primary\" dark >\n      <v-app-bar-nav-icon @click=\"dMenuLateral = true\"></v-app-bar-nav-icon>\n      <div class=\"d-flex align-center\">\n        <v-img alt=\"Trebol Series\" class=\"shrink mr-2\" contain src=\"./assets/logo.png\" transition=\"scale-transition\" width=\"40\" />\n      </div>\n\n      <v-spacer></v-spacer>\n\n      <v-btn @click=\"fOpenLoginUser\" text >\n        <span class=\"mr-2\">Iniciar sesi√≥n</span>\n        <v-icon>mdi-account</v-icon>\n      </v-btn>\n    </v-app-bar>\n    <v-navigation-drawer v-model=\"dMenuLateral\" absolute temporary>\n      <v-list v-for=\"tipo in dListaLateral\" :key=\"tipo.value\">\n          <v-list-item-title v-if=\"tipo.items === undefined\">\n            <span @click=\"fSelectTipo\">\n              {{ tipo.title }}</span>\n          </v-list-item-title>\n          <v-list-group v-else >\n            <template v-slot:activator>\n              <v-list-item-title>{{ tipo.title }}</v-list-item-title>\n            </template>\n              <v-list-item v-for=\"categoria in tipo.items\" :key=\"categoria.value\" link>\n                <v-list-item-title> {{ categoria.title }} </v-list-item-title>\n              </v-list-item>\n          </v-list-group>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-main>\n      <InitHome></InitHome>\n    </v-main>\n    <LoginUser v-if=\"dModalLogin\" :pModal=\"dModalLogin\" @close=\"fCloseLogin\"></LoginUser>\n  </v-app>\n</template>\n\n<script>\nimport InitHome from './components/InitHome.vue';\nimport LoginUser from './components/LoginUser.vue';\n\nexport default {\n  name: 'App',\n\n  components: {\n    InitHome,\n    LoginUser\n  },  \n\n  data: () => ({\n    dModalLogin: false,\n    dMenuLateral: false,\n    dListaLateral:[\n      {title: 'Series', value: 0},\n      {title: 'Peliculas', value: 1},\n      {title: 'Categorias', value: 2, \n       items: [\n        { 'title': 'Terror', value: 0},\n        { 'title': 'Comedia', value: 1},\n        { 'title': 'Drama', value: 2},\n       ]\n      }\n    ]\n  }),\n\n  methods: {\n    fOpenLoginUser(){\n      this.dModalLogin = true\n    },\n    fCloseLogin(){\n      this.dModalLogin =false\n    },\n    fSelectTipo(tipo){\n      console.log(\"Tipo\")\n      console.log(tipo)\n    }\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}