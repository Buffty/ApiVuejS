{"ast":null,"code":"import InitHome from './components/InitHome.vue';\nimport LoginUser from './components/LoginUser.vue';\nexport default {\n  name: 'App',\n  components: {\n    InitHome,\n    LoginUser\n  },\n  data: () => ({\n    dModalLogin: false,\n    dMenuLateral: false,\n    dListaLateral: []\n  }),\n  created() {\n    this.fIniciarVariables();\n  },\n  methods: {\n    fIniciarVariables() {\n      this.dListaLateral = [{\n        title: this.$t('navBar.series'),\n        value: 0\n      }, {\n        title: this.$t('navBar.peliculas'),\n        value: 1\n      }, {\n        title: this.$t('navBar.categorias'),\n        value: 2,\n        items: [{\n          'title': this.$t('categorias.terror'),\n          value: 0\n        }, {\n          'title': this.$t('categorias.comedia'),\n          value: 1\n        }, {\n          'title': this.$t('categorias.drama'),\n          value: 2\n        }]\n      }];\n    },\n    fOpenLoginUser() {\n      this.dModalLogin = true;\n    },\n    fCloseLogin() {\n      this.dModalLogin = false;\n    },\n    fSelectTipo(tipo) {\n      console.log(\"Tipo\");\n      console.log(tipo);\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA;AACA;AAEA;EACAA;EAEAC;IACAC;IACAC;EACA;EAEAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA,sBACA;QAAAC;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA,GACA;QAAAD;QAAAC;QACAC,QACA;UAAA;UAAAD;QAAA,GACA;UAAA;UAAAA;QAAA,GACA;UAAA;UAAAA;QAAA;MAEA,EACA;IACA;IACAE;MACA;IACA;IACAC;MACA;IACA;IACAC;MACAC;MACAA;IACA;EACA;AACA","names":["name","components","InitHome","LoginUser","data","dModalLogin","dMenuLateral","dListaLateral","created","methods","fIniciarVariables","title","value","items","fOpenLoginUser","fCloseLogin","fSelectTipo","console"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app color=\"primary\" dark >\n      <v-app-bar-nav-icon @click=\"dMenuLateral = true\"></v-app-bar-nav-icon>\n      \n      <v-spacer/>\n      <v-row>\n      <v-col cols=\"12\" class=\"d-flex align-center\">\n        <v-img :alt=\"$t('app.title')\" class=\"shrink mr-2\" contain src=\"./assets/logo.png\" transition=\"scale-transition\" width=\"40\" />\n        <span> {{ $t('app.title') }}</span>\n      <v-btn>{{ $t('app.tv') }}</v-btn>\n      <v-btn>{{ $t('app.person') }}</v-btn>\n      <v-btn>{{ $t('app.cinema') }}</v-btn>\n    </v-col>\n      </v-row>\n      <v-spacer></v-spacer>\n\n      <v-btn @click=\"fOpenLoginUser\" text >\n        <span class=\"mr-2\">{{ $t('loginUser.initSession') }}</span>\n        <v-icon>mdi-account</v-icon>\n      </v-btn>\n    </v-app-bar>\n    <v-navigation-drawer v-model=\"dMenuLateral\" absolute temporary>\n      <v-list class=\" ma-0 pa-0\" v-for=\"tipo in dListaLateral\" :key=\"tipo.value\">\n        <v-list-item-group class=\"ma-3\" v-if=\"tipo.items === undefined\">\n          <v-list-item>\n            <v-list-item-title>\n              <span @click=\"fSelectTipo\"> {{ tipo.title }} </span>\n            </v-list-item-title>\n          </v-list-item>\n        </v-list-item-group>\n        <v-list-group v-else class=\"ma-3\" >\n          <template v-slot:activator>\n            <v-list-item-title>{{ tipo.title }}</v-list-item-title>\n          </template>\n            <v-list-item v-for=\"categoria in tipo.items\" :key=\"categoria.value\" link>\n              <v-list-item-title> {{ categoria.title }} </v-list-item-title>\n            </v-list-item>\n        </v-list-group>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-main>\n      <InitHome></InitHome>\n    </v-main>\n    <LoginUser v-if=\"dModalLogin\" :pModal=\"dModalLogin\" @close=\"fCloseLogin\"></LoginUser>\n  </v-app>\n</template>\n\n<script>\nimport InitHome from './components/InitHome.vue';\nimport LoginUser from './components/LoginUser.vue';\n\nexport default {\n  name: 'App',\n\n  components: {\n    InitHome,\n    LoginUser\n  },  \n\n  data: () => ({\n    dModalLogin: false,\n    dMenuLateral: false,\n    dListaLateral:[]\n  }),\n  created(){\n    this.fIniciarVariables()\n  },\n  methods: {\n    fIniciarVariables(){\n      this.dListaLateral = [\n      {title: this.$t('navBar.series'), value: 0},\n      {title: this.$t('navBar.peliculas'), value: 1},\n      {title: this.$t('navBar.categorias'), value: 2, \n       items: [\n        { 'title': this.$t('categorias.terror'), value: 0},\n        { 'title': this.$t('categorias.comedia'), value: 1},\n        { 'title': this.$t('categorias.drama'), value: 2},\n       ]\n      }\n    ]\n    },\n    fOpenLoginUser(){\n      this.dModalLogin = true\n    },\n    fCloseLogin(){\n      this.dModalLogin =false \n    },\n    fSelectTipo(tipo){\n      console.log(\"Tipo\")\n      console.log(tipo)\n    }\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}