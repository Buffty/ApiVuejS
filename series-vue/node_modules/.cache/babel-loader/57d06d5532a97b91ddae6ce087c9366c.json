{"ast":null,"code":"// COMPONENTES\n\nimport InitHome from '@/components/InitHome.vue';\nimport LoginUser from '@/components/LoginUser.vue';\nimport BarraLateral from '@/components/BarraLateral.vue';\nimport FooterApp from '@/components/FooterApp.vue';\nimport DetailTv from '@/components/DetailTv.vue';\nexport default {\n  name: 'App',\n  components: {\n    InitHome,\n    LoginUser,\n    BarraLateral,\n    FooterApp,\n    DetailTv\n  },\n  data() {\n    return {\n      dModalLogin: false,\n      dMenuLateral: false,\n      dModalDetails: false,\n      dIdTv: 0\n    };\n  },\n  methods: {\n    fOpenLoginUser() {\n      this.dModalLogin = true;\n    },\n    fCloseLogin() {\n      this.dModalLogin = false;\n    },\n    fOpenDetails(idTv) {\n      this.dModalDetails = true;\n      this.dIdTv = idTv;\n    },\n    fReturnSeries() {\n      this.dModalDetails = false;\n      this.dIdTv = 0;\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA;;AAEA;AACA;AACA;AACA;AACA;AAEA;EACAA;EAEAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["name","components","InitHome","LoginUser","BarraLateral","FooterApp","DetailTv","data","dModalLogin","dMenuLateral","dModalDetails","dIdTv","methods","fOpenLoginUser","fCloseLogin","fOpenDetails","fReturnSeries"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app color=\"blue-grey darken-2\" dark >\n      <v-app-bar-nav-icon @click=\"dMenuLateral = true\"></v-app-bar-nav-icon>\n      <v-spacer/>\n      <v-row>\n        <v-col cols=\"10\" class=\"d-flex justify-space-between \">\n          <v-layout class=\"d-flex align-center\">\n            <v-img :alt=\"$t('app.title')\" class=\"shrink mr-2\" contain src=\"./assets/logo.png\" transition=\"scale-transition\" width=\"40\" />\n            <h3> {{ $t('app.title') }}</h3>\n          </v-layout>\n          <v-btn class=\"mr-2 grey darken-4 white--text text--lighten-1\">{{ $t('app.tv') }}</v-btn>\n          <v-btn class=\"mr-2 grey darken-4 white--text text--lighten-1\">{{ $t('app.person') }}</v-btn>\n          <v-btn class=\"mr-2 grey darken-4 white--text text--lighten-1\">{{ $t('app.cinema') }}</v-btn>\n        </v-col>\n      </v-row>\n      <v-spacer/>\n\n      <v-btn @click=\"fOpenLoginUser\" text >\n        <span class=\"mr-2\">{{ $t('loginUser.initSession') }}</span>\n        <v-icon>mdi-account</v-icon>\n      </v-btn>\n    </v-app-bar>\n\n   <BarraLateral :pMenuLateral=\"dMenuLateral\"></BarraLateral>\n\n    <v-main class=\"grey darken-3 d-flex align-center\">\n      <InitHome v-if=\"!dModalDetails\" @change=\"fOpenDetails\"></InitHome>\n      <DetailTv v-else :pIdTv=\"dIdTv\" @return=\"fReturnSeries\"></DetailTv>\n    </v-main>\n    <LoginUser v-if=\"dModalLogin\" :pModal=\"dModalLogin\" @close=\"fCloseLogin\"></LoginUser>\n    <FooterApp/>\n  </v-app>\n</template>\n\n<script>\n\n// COMPONENTES\n\nimport InitHome from '@/components/InitHome.vue';\nimport LoginUser from '@/components/LoginUser.vue';\nimport BarraLateral from '@/components/BarraLateral.vue';\nimport FooterApp from '@/components/FooterApp.vue';\nimport DetailTv from '@/components/DetailTv.vue';\n\nexport default {\n  name: 'App',\n\n  components: {\n    InitHome,\n    LoginUser,\n    BarraLateral,\n    FooterApp,\n    DetailTv\n  },  \n  data() {\n    return {\n      dModalLogin: false,\n      dMenuLateral: false,\n      dModalDetails: false,\n      dIdTv: 0\n    }\n  },\n  methods: {\n    fOpenLoginUser(){\n      this.dModalLogin = true\n    },\n    fCloseLogin(){\n      this.dModalLogin = false \n    },\n    fOpenDetails(idTv){\n      this.dModalDetails = true\n      this.dIdTv = idTv\n    },\n    fReturnSeries(){\n      this.dModalDetails = false\n      this.dIdTv = 0\n    }\n  }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}